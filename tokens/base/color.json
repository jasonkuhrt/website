{
  "color": {
    "base": {
      "gray": {
        "$type": "color",
        "50": { "$value": "hsl(0, 0%, 99%)" },
        "100": { "$value": "hsl(0, 0%, 97%)" },
        "200": { "$value": "hsl(0, 0%, 94%)" },
        "300": { "$value": "hsl(0, 0%, 88%)" },
        "400": { "$value": "hsl(0, 0%, 70%)" },
        "500": { "$value": "hsl(0, 0%, 50%)" },
        "600": { "$value": "hsl(0, 0%, 38%)" },
        "700": { "$value": "hsl(0, 0%, 26%)" },
        "800": { "$value": "hsl(0, 0%, 15%)" },
        "900": { "$value": "hsl(0, 0%, 8%)" },
        "950": { "$value": "hsl(0, 0%, 3%)" }
      },
      "accent": {
        "$type": "color",
        "default": { "$value": "hsl(210, 50%, 50%)", "$description": "Muted blue" },
        "dark": { "$value": "hsl(210, 50%, 40%)" }
      }
    }
  }
}

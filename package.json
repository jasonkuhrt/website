{
  "name": "kuhrt",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "check:types": "astro check",
    "check:lint": "eslint .",
    "check": "pnpm run check:format && pnpm run check:types && pnpm run check:lint",
    "lint": "eslint . --fix",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky",
    "fetch-repos": "GITHUB_TOKEN=$(gh auth token) pnpm dlx tsx scripts/fetch-repos.ts",
    "optimize-photos": "bash scripts/optimize-photos.sh"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.astro": [
      "eslint --fix",
      "prettier --write --plugin prettier-plugin-astro"
    ],
    "*.{json,md,mdx,css,html,yml,yaml}": "prettier --write"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@fontsource/space-mono": "^5.2.7",
    "@iconify-json/carbon": "^1.2.10",
    "@iconify-json/fa6-brands": "^1.2.5",
    "@tailwindcss/typography": "^0.5.16",
    "astro": "^5.14.6",
    "astro-icon": "^1.1.5",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.70",
    "@prisma-labs/prettier-config": "^0.1.0",
    "@tailwindcss/vite": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "sharp": "^0.34.2",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3"
  }
}

{
  "name": "kuhrt",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "fix:format": "dprint fmt",
    "check:format": "dprint check",
    "check:types": "tsc --noEmit",
    "check:oxlint": "oxlint",
    "fix": "pnpm run '/fix:.*/'",
    "check": "pnpm run '/check:.*/'",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "dev": "react-router dev",
    "start": "react-router dev",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' react-router build",
    "preview": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "fetch-repos": "GITHUB_TOKEN=$(gh auth token) pnpm dlx tsx scripts/fetch-repos.ts",
    "optimize-photos": "bash scripts/optimize-photos.sh",
    "cf:deploy": "pnpm build && wrangler pages deploy",
    "cf:deployments": "wrangler pages deployment list --project-name=website",
    "cf:tail": "wrangler pages deployment tail --project-name=website"
  },
  "dependencies": {
    "@fontsource/space-mono": "^5.2.9",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@react-router/dev": "^7",
    "@react-router/node": "^7",
    "@react-router/serve": "^7",
    "@tailwindcss/typography": "^0.5.19",
    "date-fns": "^4.1.0",
    "effect": "^3.18.4",
    "lucide-react": "^0.547.0",
    "react": "^19",
    "react-dom": "^19",
    "react-router": "^7.9.4",
    "vite": "^7.1.12"
  },
  "devDependencies": {
    "@effect/cluster": "^0.50.6",
    "@effect/rpc": "^0.71.1",
    "@effect/workflow": "^0.11.5",
    "@mdx-js/rollup": "^3.1.1",
    "@opentelemetry/api": "^1.9.0",
    "@playwright/test": "^1.56.1",
    "@prisma-labs/prettier-config": "^0.1.0",
    "@shikijs/rehype": "^3.13.0",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.9.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "csstype": "^3.1.3",
    "dprint": "^0.50.2",
    "glob": "^11.0.3",
    "oxlint": "^1.24.0",
    "rehype-slug": "^6.0.0",
    "remark-frontmatter": "^5",
    "remark-mdx-frontmatter": "^5",
    "sharp": "^0.34.4",
    "shiki": "^3.13.0",
    "shiki-twoslash": "^3.1.2",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "wrangler": "^4.45.0"
  }
}

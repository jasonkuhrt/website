{
  "name": "kuhrt",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "fix:format": "dprint fmt",
    "check:format": "dprint check",
    "check:types": "svelte-check --tsconfig ./tsconfig.json",
    "check:oxlint": "oxlint",
    "fix": "pnpm run '/fix:.*/'",
    "check": "pnpm run '/check:.*/'",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "dev": "vite dev",
    "histoire": "histoire dev",
    "start": "vite dev",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "fetch-repos": "GITHUB_TOKEN=$(gh auth token) pnpm dlx tsx scripts/fetch-repos.ts",
    "optimize-photos": "bash scripts/optimize-photos.sh",
    "cf:deploy": "pnpm build && wrangler pages deploy",
    "cf:deployments": "wrangler pages deployment list --project-name=website",
    "cf:tail": "wrangler pages deployment tail --project-name=website"
  },
  "dependencies": {
    "@fontsource/space-mono": "^5.2.9",
    "@iconify-json/fa6-brands": "^1.2.6",
    "@sveltejs/kit": "^2.47.3",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "bits-ui": "^2.14.1",
    "date-fns": "^4.1.0",
    "effect": "^3.18.4",
    "lucide-svelte": "^0.546.0",
    "mdsvex": "^0.12.6",
    "svelte": "^5.41.3",
    "vite": "^7.1.12"
  },
  "devDependencies": {
    "@effect/cluster": "^0.50.6",
    "@effect/rpc": "^0.71.1",
    "@effect/workflow": "^0.11.5",
    "@histoire/plugin-svelte": "1.0.0-alpha.4",
    "@iconify/svelte": "^5.0.2",
    "@opentelemetry/api": "^1.9.0",
    "@playwright/test": "^1.56.1",
    "@prisma-labs/prettier-config": "^0.1.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.9.1",
    "csstype": "^3.1.3",
    "dprint": "^0.50.2",
    "histoire": "1.0.0-alpha.4",
    "oxlint": "^1.24.0",
    "sharp": "^0.34.4",
    "shiki": "^3.13.0",
    "shiki-twoslash": "^3.1.2",
    "svelte-check": "^4.3.3",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "unplugin-icons": "^22.5.0",
    "wrangler": "^4.45.0"
  }
}
